{
  "name": "f3v",
  "main": "dist/shalotts.js",
  "module": "dist/shalotts.js",
  "version": "0.30.42",
  "description": "Fastify boilerplate for SSR Vue app",
  "author": {
    "name": "francyfox",
    "url": "https://github.com/francyfox",
    "email": "7info7web@gmail.com"
  },
  "keywords": [
    "shalotts",
    "boilerplate",
    "vue",
    "bun",
    "vike",
    "vavite"
  ],
  "repository": "https://github.com/shalotts/shalotts",
  "bugs": "https://github.com/shalotts/shalotts/issues",
  "homepage": "https://github.com/shalotts/shalotts",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "vavite dev",
    "build": "vavite-multibuild",
    "start": "node ./dist/server/index.mjs",
    "test": "jest",
    "lint": "biome lint ./app",
    "prepublish": "tsup"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/shalotts.d.ts",
        "default": "./dist/esm/shalotts.js"
      }
    },
    "./renderer/onRenderHtml": {
      "types": "./dist/onRenderHtml.d.ts",
      "default": "./dist/esm/onRenderHtml.js"
    },
    "./renderer/onRenderClient": {
      "types": "./dist/onRenderClient.d.ts",
      "default": "./dist/esm/onRenderClient.js"
    },
    "./renderer/config": {
      "types": "./dist/_config.d.ts",
      "default": "./dist/esm/+config.js"
    }
  },
  "sideEffects": [
    "dist/*"
  ],
  "files": [
    "./dist/"
  ],
  "types": "./dist/shalotts.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/shalotts.d.ts"
      ],
      "renderer/onRenderHtml": [
        "./dist/onRenderHtml.d.ts"
      ],
      "renderer/onRenderClient": [
        "./dist/onRenderClient.d.ts"
      ],
      "renderer/config": [
        "./dist/config.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/node": "^20.10.4",
    "@unhead/addons": "^1.8.10",
    "bun-types": "latest",
    "jest": "^29.7.0",
    "node-notifier": "^10.0.1",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "type": "module",
  "import": {
    "/": "/"
  },
  "dependencies": {
    "@fastify/caching": "^8.3.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/early-hints": "^1.0.1",
    "@fastify/etag": "^5.1.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.0",
    "@types/jest": "^29.5.11",
    "@unhead/ssr": "^1.8.10",
    "@vitejs/plugin-vue": "^5.0.2",
    "close-with-grace": "^1.2.0",
    "cloudflared": "^0.5.0",
    "consola": "^3.2.3",
    "defu": "^6.1.3",
    "f3v": "link:f3v",
    "fastify": "^4.25.0",
    "fastify-plugin": "^4.5.1",
    "fastify-x-request-id": "^2.0.0",
    "get-port-please": "^3.1.1",
    "isbot": "^4.3.0",
    "lightningcss": "^1.22.1",
    "nanoid": "^5.0.4",
    "postcss": "^8.4.32",
    "postcss-nested": "^6.0.1",
    "scule": "^1.1.1",
    "ts-node": "^10.9.2",
    "unhead": "^1.8.10",
    "unocss": "^0.58.0",
    "uqr": "^0.1.2",
    "vavite": "^3.0.0",
    "vike": "^0.4.158",
    "vite": "^5.0.10",
    "vite-plugin-node": "^3.1.0",
    "vitest": "^1.1.0",
    "vue": "^3.3.11"
  }
}